{% extends "base.j2" %}

{% block styles %}
<link rel="stylesheet" href="{{ url_for('static', filename='results/graphs/graph.css') }}">
{% endblock %}

{% block content %}
<div class="title">
  {{ breadcrums(["home", "Home"], ["results.index", "Results"], ["results.result", result, {"result": result}], ["results.graphs.base", "Base Graphs", {"result": result}], graph.stem) }}
  <h1>Base {{ graph.stem }}</h1>
</div>
{% include "results/graphs/graph.j2" %}
{% endblock %}

{% block postscripts %}
<script src="https://d3js.org/d3.v7.min.js"></script>
<script src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" data-auto-replace-svg="nest"></script>
<script src="{{ url_for('static', filename='results/graphs/colors.js') }}"></script>
<script src="{{ url_for('static', filename='results/graphs/scale.js') }}"></script>
<script src="{{ url_for('static', filename='results/graphs/graph.js') }}"></script>
<script src="{{ url_for('static', filename='results/graphs/navigation.js') }}"></script>
<script src="{{ url_for('static', filename='results/graphs/fetch_graph.js') }}"></script>
<script>
  fetchGraph("{{ url_for('results.graphs.base_graph_json', result=result, graph=graph.name) }}")
</script>
{% endblock %}
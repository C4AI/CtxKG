{% extends "base.j2" %}

{% block content %}
<div class="mt-12 mx-12">
    {{ breadcrums(["home", "Home"], ["results.index", "Results"], ["results.result", result, {"result": result}], "Base Graphs") }}
    <h1 class="text-4xl font-semibold mt-3">Base Graphs</h1>
</div>
<ul class="mt-5 overflow-auto">
    {% for graph in graphs %}
    <a href="{{ url_for('.base_graph', result=result, graph=graph.name) }}" class="text-decoration-none"><li class="border-b p-5 hover:text-blue-500">{{ graph.stem }}</li></a>
    {% endfor %}
</ul>
{% endblock %}

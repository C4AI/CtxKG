{% extends "base.j2" %}

{% block content %}
{{ breadcrums(["home", "Home"], ["results.index", "Results"], ["results.result", result, {"result": result}], "Base Graphs") }}
<h1>Base Graphs</h1>
<ul>
    {% for graph in graphs %}
    <li><a href="{{ url_for('.base_graph', result=result, graph=graph.name) }}">{{ graph.stem }}</a></li>
    {% endfor %}
</ul>
{% endblock %}

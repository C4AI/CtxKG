{% extends "base.j2" %}
{% from "macros/setup_graph.j2" import setup_graph %}

{% block styles %}
<link rel="stylesheet" href="{{ url_for('static', filename='results/graphs/graph.css') }}">
{% endblock %}

{% block content %}
<div class="mt-12 mx-12">
  {{ 
    breadcrums(
      ["home", "Home"],
      ["results.index", "Results"],
      ["results.result", result, {"result": result}],
      ["results.graphs.index", title, {"result": result, "version": version}],
      subtitle
    ) 
  }}
  <h1 class="text-4xl font-semibold mt-3">{{ subtitle }}</h1>
</div>
{{ setup_graph(url_for("results.graphs.graph_json", result=result, version=version, graph=graph)) }}
{% endblock %}
